{
  "name": "koa-graphql-upload",
  "version": "0.1.0",
  "description": "File upload middleware for Koa GraphQL http server ",
  "main": "dist/index.js",
  "directories": {
    "lib": "./dist"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon --exec npm run start",
    "build": "babel src -d dist",
    "lint": "eslint --fix --ext .js,.vue src",
    "test": "mocha $npm_package_options_mocha",
    "cover": "babel-node node_modules/.bin/istanbul cover --root src --report html node_modules/.bin/_mocha -- $npm_package_options_mocha",
    "cover:lcov": "babel-node node_modules/.bin/istanbul cover --root src --report lcovonly node_modules/.bin/_mocha -- $npm_package_options_mocha"
  },
  "options": {
    "mocha": "--harmony -R spec --require ./mocha-bootload ./src/**/__tests__/**/*.js"
  },
  "keywords": [
    "koa",
    "graphql",
    "koa-middleware",
    "koa-graphql"
  ],
  "author": "david.shi@easing.io",
  "license": "MIT",
  "dependencies": {
    "async-busboy": "^0.6.1",
    "babel-runtime": "^6.23.0",
    "graphql": "^0.10.3",
    "koa-graphql": "^0.7.1",
    "koa-multer": "^1.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-istanbul": "^0.12.2",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "chai": "^3.5.0",
    "coveralls": "^2.13.0",
    "eslint": "3.19.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-flowtype": "2.32.1",
    "istanbul": "^1.0.0-alpha.2",
    "koa": "^2.3.0",
    "koa-bodyparser": "^4.2.0",
    "koa-convert": "^1.2.0",
    "koa-router": "^7.2.1",
    "mocha": "^3.2.0",
    "raw-body": "^2.2.0",
    "sane": "^1.6.0",
    "supertest": "^3.0.0"
  },
  "engines": {
    "node": ">=7.0.0"
  }
}
